{
  "name": "lexatlas-website",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.11.1 <22"
  },
  "description": "LexAtlas - Your Global Legal Compass. Premium legal kits for international procedures.",
  "keywords": [
    "legal",
    "international",
    "marriage",
    "cross-border",
    "pdf",
    "kits"
  ],
  "author": "LexAtlas Team",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf .next .turbo node_modules/.cache public/.next playwright/.cache coverage",
    "prepare:world": "node scripts/vendorize-world-atlas.mjs",
    "dev": "npm run prepare:world && next dev -p 3000",
    "dev:errors": "curl -s http://localhost:3000/api/dev/errors | jq . || curl -s http://localhost:3000/api/dev/errors",
    "dev:probe": "node scripts/dev-probe.mjs",
    "dev:full": "concurrently -k -n NEXT,PROBE -c \"auto\" \"npm:dev\" \"npm:dev:probe\"",
    "dev:reset:only": "rimraf .next .turbo node_modules/.cache public/.next playwright/.cache coverage",
    "dev:reset": "npm run dev:reset:only && npm run dev",
    "dev:reset:hard": "node scripts/dev-hard-reset.mjs && npm run dev",
    "dev:safe": "cross-env NEXT_SAFE_MODE=1 next dev -p 3000",
    "dev:kill": "node scripts/kill-ports.mjs || true",
    "diagnose:chunks": "node scripts/check-next-chunks.mjs || echo 'ok'",
    "build": "npm run prepare:world && next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest --run",
    "test:unit": "vitest --run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:unit && npm run test:e2e",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "dev:seed:stripe": "tsx scripts/seed-stripe.ts",
    "generate:og:pairs": "tsx scripts/og-export.ts",
    "generate:icons": "node scripts/generate-icons.mjs",
    "verify:kits": "npx ts-node --transpile-only scripts/verify-kits.ts",
    "seo:audit": "node scripts/seo-audit.mjs",
    "test:a11y": "playwright test tests/e2e/a11y.spec.ts --reporter=list",
    "perf:audit": "node scripts/perf-audit.mjs",
    "test:robots": "node scripts/test-robots.mjs",
    "test:jsonld": "node scripts/test-jsonld.mjs",
    "test:pwa": "node scripts/test-pwa.mjs"
  },
  "dependencies": {
    "@esbuild/darwin-arm64": "^0.25.9",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@stripe/stripe-js": "^2.4.0",
    "@types/topojson-client": "^3.1.5",
    "@vercel/analytics": "^1.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "d3-geo": "^3.1.1",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.294.0",
    "next": "14.2.5",
    "next-intl": "^3.4.1",
    "next-seo": "^6.4.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "resend": "^6.0.1",
    "stripe": "^14.10.0",
    "tailwind-merge": "^2.2.0",
    "topojson-client": "^3.1.0",
    "world-atlas": "^2.0.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/eslintrc": "^3.0.0",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.40.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "axe-core": "^4.10.3",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "husky": "^8.0.3",
    "jsdom": "^26.1.0",
    "lighthouse": "^12.8.2",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "rimraf": "^6.0.1",
    "sharp": "^0.33.2",
    "tailwindcss": "^3.3.0",
    "tsx": "^4.7.0",
    "typescript": "^5",
    "vitest": "^1.0.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
