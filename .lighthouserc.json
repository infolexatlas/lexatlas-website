{
  "ci": {
    "collect": {
      "url": [
        "http://127.0.0.1:3000/",
        "http://127.0.0.1:3000/kits/fra-can"
      ],
      "numberOfRuns": 1,
      "startServerCommand": "next start --hostname 127.0.0.1 --port 3000",
      "startServerReadyPattern": "ready - started server on|Local:\\s?http://127\\.0\\.0\\.1:3000|URL:\\s?http://127\\.0\\.0\\.1:3000",
      "startServerReadyTimeout": 240000
    },
    "assert": {
      "assertions": {
        "categories:performance": ["error", {"minScore": 0.90}],
        "categories:accessibility": ["error", {"minScore": 0.95}]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}


